import org.junit.Assert;

import junit.framework.TestCase;

public class NodeTest extends TestCase {
	private Node node;
	@Override
	protected void setUp() throws Exception {
		node = new Node();
	}
	public void testGoL() throws Exception {
		Node testNode = new Node();
		testNode.setValue(4);
		node.setL_node(testNode);
		Assert.assertEquals(testNode, node.go());
	}
	public void testGoR() throws Exception {
		Node testNode = new Node();
		testNode.setValue(4);
		node.setR_node(testNode);
		Assert.assertEquals(testNode, node.go());
	}
	public void testGoD() throws Exception {
		Node testNode = new Node();
		testNode.setValue(4);
		node.setD_node(testNode);
		Assert.assertEquals(testNode, node.go());
	}
	public void testGoV() throws Exception {
		Node testNode = new Node();
		testNode.setValue(4);
		node.setD_node(testNode);
		Assert.assertEquals(4, node.go().getValue());
	}
}

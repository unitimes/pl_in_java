
public class NonStaticCal {
	int add(int a, int b) {
		return a + b;
	}
	int sub(int a, int b) {
		return a - b;
	}
	int mul(int a, int b) {
		return a * b;
	}
	int div(int a, int b) {
		return a / b;
	}
	public static void main(String[] args) {
		int a = 3;
		int b = 4;
		NonStaticCal cal = new NonStaticCal();
		System.out.println("Addition: " + a + " + " + b + " = " + cal.add(3, 4));
		System.out.println("Subtraction: " + a + " - " + b + " = " + cal.sub(3, 4));
		System.out.println("Multiply: " + a + " * " + b + " = " + cal.mul(3, 4));
		System.out.println("Divide: " + a + " / " + b + " = " + cal.div(3, 4));
	}
}

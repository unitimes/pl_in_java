package chessgame;

import static org.junit.Assert.*;

import org.junit.Test;

import chessgame.piece.Pawn;

public class NodeTest {

	@Test
	public void testIsPlacedWhenNotPlaced() {
		Node testNode = new Node(3, 3, null);
		assertEquals(false, testNode.isPlaced());
	}
	
	@Test
	public void testIsPlacedWhenPlaced() {
		Node testNode = new Node(3, 3, new Pawn());
		assertEquals(true, testNode.isPlaced());
	}

}

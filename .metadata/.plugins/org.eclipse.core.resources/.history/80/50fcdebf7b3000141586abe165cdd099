package chessgame.piece;

import java.util.ArrayList;

import chessgame.Board;
import chessgame.Node;

public class Pawn implements Piece {

	@Override
	public ArrayList<Node> searchMovibleNodes(int row, int col, Board presentBoard) {
		ArrayList<Node> movibleNodes = new ArrayList<Node>();
		if(presentBoard.isPlaced(row + 1, col)) {
			movibleNodes.add(new Node(row + 1, col, null));
		}
		// TODO Auto-generated method stub
		return null;
	}

}

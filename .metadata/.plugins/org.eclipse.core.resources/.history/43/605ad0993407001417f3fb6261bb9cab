public class SimpleCalculator {
	static int add(int a, int b) {
		return a + b;
	}
	public static void main(String[] args) {
		System.out.println("덧셈: " + SimpleCalculator.add(1, 3));
		System.out.println("덧셈: " + Test.add(1, 3));
	}
}
class Test {
	static int add(int a, int b) {
		return a + b;
	}
	public static void main(String[] args) {
		System.out.println("덧셈: " + SimpleCalculator.add(1, 3));
		System.out.println("덧셈: " + Test.add(1, 3));
	}
}
package chessgame;

import static org.junit.Assert.*;

import java.util.ArrayList;

import org.junit.Test;

import core.Coordinate;

public class BoardTest {

	@Test
	public void testSearchMovibleNodesWhenPlaced() {
		Board board = new Board();
		board.setPawn(3, 3);
		board.setPawn(4, 4);
		board.setPawn(4, 2);
		board.searchMovibleGrids(3, 3);
	}
	
	@Test
	public void testSearchMovibleNodesWhenNotPlaced() {
		Board board = new Board();
		board.setPawn(3, 3);
		board.searchMovibleGrids(3, 3);
	}
}

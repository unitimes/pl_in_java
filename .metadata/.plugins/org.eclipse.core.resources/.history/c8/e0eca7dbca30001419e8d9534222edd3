package core;

public class Coordinate {
	int row;
	int col;
	
	public Coordinate (int row, int col) {
		this.row = row;
		this.col = col;
	}
	
	public Coordinate (String grid) {
		
	}
	
	private void setCoordinate (String grid)
	{
		String rowCoordinate = grid.substring(0, 1);
		String colCoordinate = grid.substring(1, 2);
		
		row = Integer.parseInt(rowCoordinate);
		
		if (colCoordinate.equals("a") || colCoordinate.equals("A")) {
			col = 0;
			return;
		}
		if (colCoordinate.equals("b") || colCoordinate.equals("B")) {
			col = 1;
			return;
		}
		if (colCoordinate.equals("c") || colCoordinate.equals("C")) {
			col = 2;
			return;
		}
		if ()
	}
}
